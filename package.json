{
  "name": "pm-ms-poc",
  "version": "1.0.0",
  "description": "Just trying to proof of concept out some angular 2 structure along with something for automation and continuous deployment",
  "main": "index.js",
  "scripts": {
    "build:prod": "npm run clean:dist&& npm run webpack:prod",
    "build:dev": "npm run webpack:dev",
    "clean:dist": "npm run rimraf -- dist",
    "coverage-html": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "docs": "node_modules/.bin/compodoc -p tsconfig.json -d docs&& npm run serve-docs",
    "lint": "tslint ./src/**/*.ts",
    "serve-docs": "node_modules/.bin/compodoc -s -d docs",
    "rimraf": "rimraf",
    "postinstall": "npm i rimraf -g&& npm i karma -g",
    "start": "npm run build:dev",
    "test": "karma start --single-run true --auto-watch false",
    "test:dev": "set browser=true&& karma start",
    "test:coverage": "set browser=true&& karma start --single-run true --auto-watch false && npm run coverage-html",
    "webpack:dev": "webpack-dev-server --config webpack.config.dev.js --progress",
    "webpack:prod": "set ENV=Prod&& webpack --config webpack.config.prod.js --progress --profile --bail",
    "webpack": "webpack"
  },
